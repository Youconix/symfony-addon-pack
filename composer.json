{
    "name": "darkwebdesign/symfony-addon-pack",
    "type": "library",
    "description": "Symfony Add-on Pack",
    "keywords": ["constraints", "symfony", "validators"],
    "homepage": "http://www.darkwebdesign.nl/",
    "license": "MIT",
    "authors": [
        {"name": "Raymond Schouten"}
    ],
    "require": {
        "php": "^5.3.2",
        "symfony/validator": "2.0.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8",
        "codeclimate/php-test-reporter": "^0.3.2"
    },
    "replace": {
        "darkwebdesign/symfony-addon-validators": "1.0.0"
    },
    "autoload": {
        "psr-4": {
            "DarkWebDesign\\SymfonyAddon\\Validator\\": "symfony-addon-validators/src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DarkWebDesign\\SymfonyAddon\\Validator\\Tests\\": "symfony-addon-validators/tests/"
        }
    }
}
